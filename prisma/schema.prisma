model Restaurant {
  id          Int      @id @default(autoincrement())
  name        String
  address     String?
  city        String?
  state       String?
  zip         String?
  lat         Float
  lng         Float
  cuisine     String
  price_range String?
  rating      Float?
  phone       String?
  website     String?
  created_at  DateTime @default(now())
  updated_at  DateTime @updatedAt
}

model CoffeeShop {
  id          Int      @id @default(autoincrement())
  name        String
  image       String?
  description String?
  lat         Float
  lng         Float
  rating      Float?
  cuisine     String   @default("Coffee & Tea")
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
